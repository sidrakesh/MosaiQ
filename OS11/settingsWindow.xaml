<Window x:Name="settingWindow" x:Class="OS11.settingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Settings" Height="200" Width="248.508" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <RadioButton x:Name="javaOption" GroupName="algorithm" Content="Java implementation" HorizontalAlignment="Left" Height="21" Margin="59,66,0,0" VerticalAlignment="Top" Width="172"/>
        <RadioButton x:Name="pythonOption" GroupName="algorithm" Content="Python Implementation" HorizontalAlignment="Left" Height="21" Margin="59,87,0,0" VerticalAlignment="Top" Width="172"/>
        <Label Content="Select algorithm implementation: " HorizontalAlignment="Left" Height="27" Margin="23,28,0,0" VerticalAlignment="Top" Width="220"/>
        <Button Content="OK" HorizontalAlignment="Left" Height="30" Margin="75,122,0,0" VerticalAlignment="Top" Width="94" Click="Button_Click"/>
    </Grid>
</Window>
